{
  "name": "mango",
  "version": "1.0.0",
  "description": "A MEAN (Mongo Express Angular Node) template with some GraphQL flavour and giusseppe for routing in express",
  "main": "index.js",
  "scripts": {
    "ng": "ng",
    "build": "rimraf dist && ng build --prod --aot --base-href ./ && tsc --project server/tsconfig.json",
    "start": "pm2 start ./dist/server/server.js --no-daemon",
    "dev": "concurrently \"npm run backend\" \"npm run client\" ",
    "client": "ng serve --progress false --proxy-config proxy.conf.json",
    "backend": "nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrARC/Mango.git"
  },
  "keywords": [
    "mean",
    "mann",
    "graphql-mean",
    "typescript",
    "mongomean",
    "angular",
    "nest.js"
  ],
  "author": "Alfonso Reyes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MrARC/Mango/issues"
  },
  "homepage": "https://github.com/MrARC/Mango#readme",
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "body-parser": "^1.18.3",
    "core-js": "^2.5.4",
    "express": "^4.16.3",
    "giuseppe": "^3.1.0",
    "mongoose": "^5.1.7",
    "morgan": "^1.9.0",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/express": "^4.16.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "concurrently": "^3.6.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.17.5",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
